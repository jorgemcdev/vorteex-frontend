{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions":{
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "experimentalDecorators": true,
            "classes": true
        }
    },    
    "plugins": [
        "react"
    ],
    "rules":{
        "comma-dangle": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "no-undef": 0,
        "import/no-extraneous-dependencies": 0
    }
}